    "forever": "^1.0.0",
    "helmet": "^3.20.0",
    "lit-element": "^2.2.1",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.3",
    "socket.io": "^2.2.0",
    "useragent": "^2.3.0"
  },
  "devDependencies": {
    "polymer-cli": "^1.9.11"
  }
