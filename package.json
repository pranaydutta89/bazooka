{
  "name": "bazooka",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ui": "polymer serve",
    "buildes5": "polymer build --preset es5-bundled",
    "buildes6": "polymer build --preset uncompiled-unbundled",
    "prebuild": "rimraf dist",
    "build": "npm run buildes5 && ncp build dist && npm run buildes6 && ncp build dist",
    "postbuild": "ncp beanstalk dist && ncp package.json dist/package.json && ncp src/server dist && npm run cleanDist",
    "cleanDist": "rimraf dist/es5-bundled/src/server && rimraf dist/uncompiled-unbundled/src/server",
    "stopServer": "npm run forever stop app.js",
    "start": "npm run stopServer || npm run startServer",
    "startServer": "npm run forever start app.js",
    "forever": "forever"
  },
  "author": "Pranay Dutta",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "forever": "^1.0.0",
    "helmet": "^3.20.0",
    "lit-element": "^2.2.1",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.3",
    "socket.io": "^2.2.0",
    "useragent": "^2.3.0"
  },
  "devDependencies": {
    "prettier": "^1.18.2",
    "eslint": "^6.2.2",
    "husky": "^3.0.4",
    "polymer-cli": "^1.9.11",
    "pretty-quick": "^1.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
