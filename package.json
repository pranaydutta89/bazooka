{
  "name": "tugofwar",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ui": "polymer serve",
    "buildes5": "polymer build --preset es5-bundled",
    "buildes6": "polymer build --preset uncompiled-unbundled",
    "build": "npm run clean && npm run buildes5 && ncp build dist && npm run buildes6 && ncp build dist && npm run copy",
    "copy": "ncp beanstalk dist && ncp package.json dist/package.json && ncp server dist",
    "clean": "rimraf dist",
    "stopServer": "forever stop server/server.js",
    "start": "npm run stopServer || npm run startServer",
    "startServer": "forever start server/server.js"
  },
