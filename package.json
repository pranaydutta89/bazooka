{
  "name": "bazooka",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ui:build": "parcel build src/app/index.html --no-cache",
    "ui:watch": "parcel src/app/index.html --no-cache",
    "prebuild": "rimraf dist",
    "build": "polymer build && ncp build dist",
    "postbuild": "ncp beanstalk dist && ncp package.json dist/package.json && ncp src/server dist && npm run cleanDist",
    "cleanDist": "rimraf dist/es5-bundled/src/server && rimraf dist/uncompiled-bundled/src/server",
    "stopServer": "npm run forever stop app.js",
    "start": "npm run stopServer || npm run startServer",
    "startServer": "npm run forever start app.js",
    "forever": "forever"
  },
  "author": "Pranay Dutta",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "bootstrap": "^4.3.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "forever": "^1.0.0",
    "gulp": "^4.0.2",
    "helmet": "^3.20.0",
    "images": "^3.0.2",
    "lit-element": "^2.2.1",
    "navigo": "^7.1.2",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.3",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "useragent": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^6.2.2",
    "husky": "^3.0.4",
    "polymer-cli": "^1.9.11",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "sass": "^1.23.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
